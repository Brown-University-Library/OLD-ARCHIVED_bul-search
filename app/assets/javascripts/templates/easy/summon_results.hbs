<ul class="list-unstyled">
    {{#each docs}}
    <li>
        <a href="{{this.link}}">{{this.title}}</a>
        <br/>
        {{this.author}}. <em>{{ this.venue }}</em> {{this.year}} {{#if this.volume }}{{ this.volume }}{{/if}}{{#if this.issue }}:{{ this.issue }}{{/if}}{{#if this.start }} pg. {{this.start}}- {{/if}}.
    </li>
    {{/each}}
</ul>
<div class="more center-block">
    <a href="{{this.more}}">View {{this.total}} results</a>
</div>
