<ul class="list-unstyled">
    {{#each docs}}
    <li>
        <a href="{{this.link}}">{{truncate this.title ../max_title_length}}</a>
        <br/>
        <small>
            {{#if this.author }}{{this.author}}.&nbsp;{{/if}}
            {{#if this.year }}{{this.year}}{{/if}}
        <br/>
        <em>{{this.venue}}</em>.&nbsp;{{#if this.volume }}Vol: {{ this.volume }}.&nbsp;{{/if}}{{#if this.issue }}Issue: {{ this.issue }}.&nbsp;{{/if}}{{#if this.start }}pg. {{this.start}}{{/if}}.</small>
    </li>
    {{/each}}
</ul>
<div class="more center-block">
    <a href="{{this.more}}">View {{this.total}} results</a>
</div>
