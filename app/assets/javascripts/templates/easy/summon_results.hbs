<ul class="list-unstyled">
    {{#each docs}}
    <li>
        <a href="{{this.link}}">{{this.title}}</a>
        <br/>
        {{this.author}}. <em>{{ this.venue }}</em> {{#if this.volume }}{{ this.volume }}{{/if}}{{#if this.issue }}:{{ this.issue }}{{/if}} {{this.year}}.
    </li>
    {{/each}}
</ul>
<div class="more center-block">
    <a href="{{this.more}}">View {{this.total}} results</a>
</div>
