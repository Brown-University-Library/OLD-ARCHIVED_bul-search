{{#if this.other}}
    <h3 class="bento-header">{{#if this.icon}}<i class="fa fa-{{this.icon}}"></i>{{/if}}{{this.box}}</h3>
{{/if}}
<ul class="list-unstyled">
    {{#each docs}}
    <li>
        <a href="{{this.link}}">{{this.title_display}}</a>
        <br/>
        <small>
            {{#if this.author_display}}{{this.author_display}}.&nbsp;{{/if}}
            {{#if this.pub_date }}{{this.pub_date}}{{/if}}
        {{#if this.online }}
            <br/>
            <a href="{{this.link}}" class="label label-info">Online</a>
        {{/if}}</small>
    </li>
    {{/each}}
</ul>
<div class="more center-block">
    <a href="{{this.more}}">View {{this.total}} results</a>
</div>
