<div style="overflow-x: scroll;">
  <table class="table">
      <tr class="row">
        <th>#</th>
        <th>BIB</th>
        <th>Item</th>
        <th>Title</th>
        <th>Pub Year</th>
        <th>Publisher</th>
        <th>Location</th>
        <th>Checkout (All)</th>
        <th>Checkouts (2015+)</th>
        <th>Bib Created</th>
        <th>Bib Cataloged</th>
        <th>Item Created</th>
        <th>Call number</th>
        <th>Subjects</th>
      </tr>
      <% @presenter.rows.each_with_index do |r, ix| %>
        <tr class="row">
          <td><%= ix + 1 %></td>
          <td><a href="<%= catalog_url( r.josiah_bib_id ) %>"><%= r.josiah_bib_id %></a></td>
          <td><%= r.item_record_num %></td>
          <td><%= r.title %></td>
          <td><%= r.publish_year %></td>
          <td><%= r.publisher %></td>
          <td><%= r.location_code %></td>
          <td><%= r.checkout_total %></td>
          <td><%= r.checkout_2015_plus %></td>
          <td><%= r.date_display(r.bib_create_date) %></td>
          <td><%= r.date_display(r.bib_catalog_date) %></td>
          <td><%= r.date_display(r.item_create_date) %></td>
          <td class="callnumber"><%= r.callnumber_raw %></td>
          <td>
            <%= r.subjects_display %>
            <% if r.subjects_tooltip? %>
              <span title="<%= r.subjects_tooltip %>">...</span>
            <% end %>
          </td>
        </tr>
      <% end %>
  </table>
</div>